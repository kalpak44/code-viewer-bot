{
  "name": "code-viewer-bot",
  "displayName": "Code Viewer Bot",
  "description": "A joke extension that randomly opens files and moves the cursor.",
  "version": "0.0.1",
  "publisher": "kalpak44",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kalpak44/code-viewer-bot.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "dependencies": {
    "@nut-tree-fork/nut-js": "^4.2.6"
  },
  "devDependencies": {
    "vsce": "^2.11.0"
  },
  "scripts": {
    "postinstall": "echo \"Rebuilding native modules for nut-js...\" && npm rebuild @nut-tree-fork/nut-js || true"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.runBot",
        "title": "Run Bot"
      },
      {
        "command": "extension.stopBot",
        "title": "Stop Bot"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.runBot",
          "when": "!botRunning",
          "group": "navigation"
        },
        {
          "command": "extension.stopBot",
          "when": "botRunning",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "extension.runBot",
          "when": "!botRunning",
          "group": "navigation"
        },
        {
          "command": "extension.stopBot",
          "when": "botRunning",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "extension.runBot",
          "when": "!botRunning"
        },
        {
          "command": "extension.stopBot",
          "when": "botRunning"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.stopBot",
        "key": "escape",
        "when": "botRunning"
      }
    ]
  }
}